# Configuración del Sistema Cancha Inteligente

# API Keys (opcional - para funcionalidades avanzadas)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=tu_api_key_aqui

# Configuración GPS
NEXT_PUBLIC_GPS_ACCURACY=3
NEXT_PUBLIC_GPS_TIMEOUT=10000
NEXT_PUBLIC_GPS_MAX_AGE=0

# Configuración FIFA
NEXT_PUBLIC_FIFA_LENGTH_MIN=100
NEXT_PUBLIC_FIFA_LENGTH_MAX=110
NEXT_PUBLIC_FIFA_WIDTH_MIN=64
NEXT_PUBLIC_FIFA_WIDTH_MAX=75
NEXT_PUBLIC_FIFA_TOLERANCE=5

# Configuración de la Cancha
NEXT_PUBLIC_CANCHA_LENGTH=105
NEXT_PUBLIC_CANCHA_WIDTH=68
NEXT_PUBLIC_CANCHA_AREA=7140

# Configuración de Productos
NEXT_PUBLIC_PRODUCTOS_ACTUALIZACION=86400000

# Configuración de Reportes
NEXT_PUBLIC_REPORTE_FORMATO=json
NEXT_PUBLIC_REPORTE_INCLUIR_GPS=true 